!!!
%html
  %head
    %title CultivatedCommerce
    = javascript_include_tag 'application', 'data-turbolinks-track' => true
    = javascript_include_tag '//ajax.googleapis.com/ajax/libs/jqueryui/1.9.1/jquery-ui.min.js', 'data-turbolinks-track' => true
    = javascript_include_tag '//maps.google.com/maps/api/js?v=3.13&amp;sensor=false&amp;libraries=geometry', 'data-turbolinks-track' => true
    = javascript_include_tag '//google-maps-utility-library-v3.googlecode.com/svn/tags/markerclustererplus/2.0.14/src/markerclusterer_packed.js', 'data-turbolinks-track' => true
        
    = stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true
    = stylesheet_link_tag    '//maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css', 
    media: 'all', 'data-turbolinks-track' => true
    = stylesheet_link_tag    'http://fonts.googleapis.com/css?family=Open+Sans:400,600,700,800', media: 'all', 'data-turbolinks-track' => true
    
    = csrf_meta_tags
  %body
    =render 'layouts/header'    
    .container-fluid.nopadding
      .col-md-12.nopadding
        = yield